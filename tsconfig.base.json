{
    "compilerOptions": {
        "importHelpers": true,
        "noEmitOnError": true,
        "moduleResolution": "Node16",
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "module": "Node16",
        "target": "ES2017",
        "paths": {
            "@microsoft/kiota-abstractions": [
                "./packages/abstractions"
            ],
        },
    }
}